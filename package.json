{
  "name": "flappybird-clone",
  "version": "0.0.1",
  "description": "Flappybird game clone",
  "main": "src/server/index.js",
  "scripts": {
    "postinstall": "NODE_ENV=production ./node_modules/.bin/webpack",
    "start": "node ./src/server/index.js",
    "dev:server": "NODE_ENV=development ./node_modules/.bin/nodemon -w ./src/server ./src/server/index.js",
    "dev:client": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server",
    "test": "./node_modules/.bin/mocha ./test --reporter spec",
    "test:game": "./node_modules/.bin/mocha ./test-game --reporter spec",
    "lint": "./node_modules/.bin/eslint webpack.config.js webpack src test test-game"
  },
  "author": "Fela Maslen",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.4",
    "ejs": "^2.5.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "immutable": "^3.8.1",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^3.1.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-create-reducer": "^1.1.1",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "webpack": "^3.5.5"
  },
  "devDependencies": {
    "eslint": "^4.5.0",
    "nodemon": "^1.11.0",
    "webpack-dev-server": "^2.7.1"
  }
}
